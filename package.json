{
  "name": "styled-by",
  "version": "0.3.0",
  "description": "Simple and powerful lib to handle styled props in your components",
  "main": "dist/index.js",
  "repository": "github.com/brunobertolini/styled-by",
  "license": "MIT",
  "author": {
    "name": "Bruno Bertolini",
    "email": "dev@brunobertolini.com",
    "url": "http://brunobertolini.com"
  },
  "scripts": {
    "prepare": "yarn husky install",
    "fix": "run-s \"fix:* {@}\" --",
    "fix:lint": "eslint --fix --no-error-on-unmatched-pattern",
    "fix:format": "prettier --write",
    "format": "yarn fix:format 'packages/**/*.(js|ts|md|json)'",
    "lint": "yarn fix:lint 'packages/**/*.{js,ts,json}'",
    "test": "yarn jest",
    "build": "yarn tsc"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "react",
    "css",
    "css-in-js",
    "styled-components",
    "props"
  ],
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/jest": "^26.0.24",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  }
}
